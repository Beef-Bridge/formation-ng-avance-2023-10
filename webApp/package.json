{
  "name": "web-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port=3000",
    "start-en": "ng serve --open --port=3000 --configuration=page_en",
    "start-es": "ng serve --open --port=3000 --configuration=page_es",
    "build": "ng build",
    "build-localize": "ng build --localize",
    "watch": "ng build --watch --configuration development  --stats-json",
    "test": "ng test --code-coverage",
    "json-server": "json-server -p 3001 src/assets/json/bdd.json ",
    "analyze": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.2.8",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.2.8",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@angular/service-worker": "^16.1.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "rxfire": "^6.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular/cli": "~16.1.3",
    "@angular/compiler-cli": "^16.1.0",
    "@angular/localize": "^16.1.3",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}